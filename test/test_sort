int main() {
    int a[10] = {
        32, 24, 6, 15, 44,
        94, 3, 54, 86, 21
    };
    int tmp, i, j;
    for (i = 0; i < 10; i = i+1) {
      for (j = 0; j < 10-1-i; j = j+1) {
        if (a[j] > a[j+1]) {
          tmp = a[j];
          a[j] = a[j+1];
          a[j+1] = tmp;
        }
      }
    }
    for (i = 0; i < 10; i = i+1) {
      putint(a[i]);
    }
    return 0;
}
